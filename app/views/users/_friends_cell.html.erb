<tbody>
  <tr>
    <th scope="row"><%= friend.email %></td>
    <td><%= friend.first_name %></td>
    <td><%= friend.last_name %></td>
    <td>
       <%= link_to 'View Profile', user_path(friend), class: "btn btn-sm btn-success" %>
      <%= link_to 'Unfollow', friendship_path(friend), method: :delete, class: "btn btn-sm btn-danger", data: {confirm: "Are you sure you want to untrack"} %>
    </td>
  </tr>
</tbody>